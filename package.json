{
  "name": "vscode-model-viewer",
  "displayName": "glTF Model Viewer",
  "description": "Wrapper of google's model-viewer",
  "publisher": "cloudedcat",
  "author": {
		"name": "Maksim Fedorov",
		"email": "cloudecat@gmail.com"
	},
  "icon": "logo.png",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.74.0"
  },
	"repository": {
		"type": "git",
		"url": "https://github.com/cloudedcat/vscode-model-viewer"
	},
  "bugs": {
    "url": "https://github.com/cloudedcat/vscode-model-viewer/issues"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [],
  "keywords": [
    "gltf",
    "glb"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "modelViewer.gltfPreview",
        "displayName": "glTF",
        "selector": [
          {
            "filenamePattern": "*.{glb,gltf,vrm}"
          }
        ],
        "priority": "option"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@google/model-viewer": "^2.1.1",
    "@types/vscode-webview": "^1.57.1"
  }
}
